# API Keys - Replace with your real API keys
ANTHROPIC_API_KEY=your_real_anthropic_api_key_here
OPENAI_API_KEY=your_real_openai_api_key_here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/aioptimization
REDIS_URL=redis://localhost:6379

# Environment Settings
ENVIRONMENT=production
USE_REAL_TRACKING=true
ENABLE_REAL_TRACKING=true

# Authentication
JWT_SECRET_KEY=your_jwt_secret_key_here
GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id
GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password
NOTIFICATION_FROM_EMAIL=your_email@gmail.com
